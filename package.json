{
  "name": "saveapp",
  "version": "1.0.0",
  "description": "Application de sauvegarde automatis√©e pour utilisateurs non-techniques",
  "main": "dist/main/index.js",
  "scripts": {
    "dev": "electron-vite dev",
    "build": "electron-vite build",
    "preview": "electron-vite preview",
    "typecheck": "tsc --noEmit",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx}\" \"electron/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx}\" \"electron/**/*.ts\"",
    "ci": "npm run typecheck && npm run lint && npm run format:check",
    "package": "npm run build && electron-builder --config electron-builder.config.js"
  },
  "keywords": [
    "backup",
    "electron",
    "saveapp"
  ],
  "author": "Vincent",
  "license": "MIT",
  "devDependencies": {
    "@electron-toolkit/utils": "^4.0.0",
    "@types/node": "^25.0.3",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@typescript-eslint/eslint-plugin": "^8.51.0",
    "@typescript-eslint/parser": "^8.51.0",
    "@vitejs/plugin-react": "^5.1.2",
    "autoprefixer": "^10.4.23",
    "electron": "^39.2.7",
    "electron-builder": "^26.0.12",
    "electron-store": "^11.0.2",
    "electron-vite": "^5.0.0",
    "eslint": "^8.57.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^4.6.2",
    "postcss": "^8.5.6",
    "prettier": "^3.7.4",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "tailwindcss": "^3.4.0",
    "typescript": "^5.9.3",
    "vite": "^7.3.0"
  },
  "dependencies": {
    "googleapis": "^169.0.0",
    "lucide-react": "^0.562.0",
    "sonner": "^2.0.7"
  }
